<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    </head>

    <body>
        <div class="container">
            <h2>Danh Sach Sinh Vien</h2>
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Ten Sinh Vien</th>                        
                    </tr>
                </thead>
                <tbody>
                    <% _.forEach(data.Items, function(value) { %>
                    <tr>
                        <td><%= value.id %></td>
                        <td><%= value.tenSV %></td>
                        <td><a href="/sinhvien/delete/<%= value.id %>" class="btn btn-danger">Delete</a>
                            <a href="/sinhvien/update/<%= value.id %>" class="btn btn-info">Update</a>
                        </td> 
                    </tr>
                    <% }) %>
    
    
                </tbody>
            </table>
            <a href="/sinhvien/add" class="btn btn-success">Them Sinh Vien</a>
            
        </div>
    </body>
</html>